<section class="status">
    <div class="container mx-auto">
        <div class="status-box w-full lg:W-2/3 mx-auto">
            <div class="flex items-center justify-between mb-12">
                <h1 class="text-xl fint-bold">Track Delivery Status</h1>
                <h6 class="bg-white py-1 rounded-full px-4 text-green-600 text-x"><%= order._id %> </h6>
                <input id="hiddenInput" type="hidden" value="<%= JSON.stringify(order) %> ">
            </div>
            <ul>
                <li class="status_line text-sm md:text-xl pb-16" data-status="order place"><span>Order Place</span></li>
                <li class="status_line text-sm md:text-xl pb-16" data-status="confirmed"><span>Order Confirmation</span></li>
                <li class="status_line text-sm md:text-xl pb-16" data-status="prepared"><span>Prepration</span></li>
                <li class="status_line text-sm md:text-xl pb-16" data-status="delivered"><span>Out For Delivery</span></li>
                <li class="status_line text-sm md:text-xl" data-status="Completed"><span>Completed</span></li>
            </ul>
        </div>
    </div>
</section>